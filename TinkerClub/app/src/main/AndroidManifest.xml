<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dardevpro.tinkerclub">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="androidx.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:icon="@drawable/tinkerclub_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.TinkerClub.NoActionBar">
        <activity android:name=".activiter.ChatActivity">
        </activity>
        <activity android:name=".activiter.UserInfoActivity">
        </activity>
        <activity android:name=".activiter.SignupActiviter"></activity>
        <activity android:name=".activiter.loginActiviter" />
        <activity android:name=".activiter.DemActiviter">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activiter.TinkerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>